    <LCIAMethodInformation>
        <dataSetInformation>
            <common:UUID>682e1644-978e-49d7-afa6-a91cf3312840</common:UUID>
            <common:name xml:lang="en">ILCD2011; Land use; endpoint; PDF; ReCiPe</common:name>
            <methodology>ILCD2011</methodology>
            <methodology>ReCiPe2008</methodology>
            <impactCategory>Land use</impactCategory>
            <areaOfProtection>Natural environment</areaOfProtection>
            <impactIndicator>Potentially Disappeared Fraction of species (PDF) expressing the loss of species caused by land transformation and occupation.</impactIndicator>
            <common:generalComment xml:lang="en">Indicator based on biodiversity.</common:generalComment>
            <referenceToExternalDocumentation type="source data set" refObjectId="4955b2e6-e1ee-4d7e-90a2-d412fb747b4f" uri="../sources/4955b2e6-e1ee-4d7e-90a2-d412fb747b4f.xml">
                <common:shortDescription xml:lang="en">De Schryver et al. (2008)</common:shortDescription>
            </referenceToExternalDocumentation>
        </dataSetInformation>
        <quantitativeReference>
            <referenceQuantity type="flow property data set" refObjectId="feebd083-25b0-43d6-965e-543777e711f0" uri="../flowproperties/feebd083-25b0-43d6-965e-543777e711f0.xml">
                <common:shortDescription xml:lang="en">Potentially Disappeared Number of plant species in terrestrial ecosystems * time</common:shortDescription>
            </referenceQuantity>
        </quantitativeReference>
        <time>
            <referenceYear xml:lang="en">time independent</referenceYear>
            <duration xml:lang="en">Indefinite</duration>
        </time>
        <geography>
            <interventionLocation>WEU</interventionLocation>
            <impactLocation>WEU</impactLocation>
            <geographicalRepresentativenessDescription xml:lang="en">Applicable to world. The effects that appear on the occupied/used area (definded as &quot;local effects&quot;) and the effects on the surrounded area (reduced natural area; defined as &quot;regional effects&quot;) are taken into account.</geographicalRepresentativenessDescription>
        </geography>
        <impactModel>
            <modelName>None</modelName>
            <modelDescription xml:lang="en">The characterization model reflects the loss of species based on species area relationship. 18 different, relevant land use types for occupation (including three types of intensiveness for arable areas) + 4 land conversions. The factors have been mapped to a elementary flow list derived from the UNEP/SETAC working group on land use, status September 2010.</modelDescription>
            <referenceToModelSource type="source data set" refObjectId="4955b2e6-e1ee-4d7e-90a2-d412fb747b4f" uri="../sources/4955b2e6-e1ee-4d7e-90a2-d412fb747b4f.xml">
                <common:shortDescription xml:lang="en">De Schryver et al. (2008)</common:shortDescription>
            </referenceToModelSource>
            <referenceToModelSource type="source data set" refObjectId="76858297-7386-4fb2-b6f0-499bbfe171a1" uri="../sources/76858297-7386-4fb2-b6f0-499bbfe171a1.xml">
                <common:shortDescription xml:lang="en">Köllner (2001)</common:shortDescription>
            </referenceToModelSource>
            <consideredMechanisms xml:lang="en">Characterisation model reflects loss of species based on species area relationship. Considers three types of land use intensity for arable land.  Excludes effects on primary production.</consideredMechanisms>
            <referenceToMethodologyFlowChart type="source data set" refObjectId="0e568d14-207f-4833-b366-59a6080bd352" uri="../sources/0e568d14-207f-4833-b366-59a6080bd352.xml">
                <common:shortDescription xml:lang="en">Land_use_figure</common:shortDescription>
            </referenceToMethodologyFlowChart>
        </impactModel>
    </LCIAMethodInformation>
